# use CocoaPods to install AnyChatSDK
platform :ios, '13.0'
use_frameworks!

target 'AnyChatSDKExample' do
  # Use local development pod (comment out for published pod)
  pod 'AnyChatSDK', :path => '../'

  # Uncomment to use published pod:
  # pod 'AnyChatSDK', '~> 0.1'
end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '13.0'
      config.build_settings['SWIFT_VERSION'] = '5.9'
    end
  end
end
